<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="azul1">#13325C</color>
    <color name="azul2">#134874</color>
    <color name="azul3">#24659B</color>
    <color name="azul4">#3482C0</color>
    <color name="azul5">#46B2E2</color>
    <color name="black">#FF000000</color>
    <color name="blackShadow">#302728</color>
    <color name="gray">#737070</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="watergreen">#4CB5AB</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">Home</string>
    <style name="Theme.AppZonaAzul" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/azul1</item>
        <item name="colorPrimaryVariant">@color/azul1</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.AppZonaAzul.NoActionBar" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:statusBarColor" ns1:targetApi="l">@color/black</item>
    </style>
</resources>